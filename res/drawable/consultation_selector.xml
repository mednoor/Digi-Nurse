<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Button Disable -->
    <item android:drawable="@drawable/button_disable" android:state_enabled="false"/>
    <!-- Button Focused -->
    <item android:drawable="@drawable/consultation_hover" android:state_focused="true" android:state_pressed="false"/>
    <!-- Button Focused Pressed -->
    <item android:drawable="@drawable/consultation_hover" android:state_focused="true" android:state_pressed="true"/>
    <!-- Button Pressed -->
    <item android:drawable="@drawable/consultation_hover" android:state_focused="false" android:state_pressed="true"/>
    <!-- Button Default Image -->
    <item android:drawable="@drawable/consultation_bg"/>

</selector>